{
  "name": "argon",
  "version": "4.0.0",
  "author": "Google Inc.",
  "license": "Apache-2.0",
  "description": "Google Marketing Platform Reporting to BigQuery connector",
  "homepage": "https://github.com/google/argon#readme",
  "repository": {
    "type": "git",
    "url": "https://github.com/google/argon.git"
  },
  "main": "./src/index.js",
  "engines": {
    "node": ">=16"
  },
  "scripts": {
    "start": "functions-framework --target=argon",
    "watch": "concurrently \"tsc -w\" \"nodemon --watch ./dist/ --exec 'cd ./dist/ && npm run start'\"",
    "lint": "gts lint",
    "format": "gts fix",
    "clean": "gts clean",
    "prebuild": "npm run clean",
    "build": "tsc",
    "test": "echo 'Error: No tests defined' && exit 1"
  },
  "dependencies": {
    "@google-cloud/bigquery": "^6.0.1",
    "@google-cloud/functions-framework": "^3.1.2",
    "google-auth-library": "^8.1.1",
    "got": "^12.2.0",
    "split2": "^3.2.2"
  },
  "devDependencies": {
    "@types/express": "^4.17.13",
    "@types/node": "^18.6.1",
    "@types/split2": "^3.2.1",
    "concurrently": "^7.3.0",
    "gts": "^4.0.0",
    "nodemon": "^2.0.19",
    "ts-node": "^10.9.1",
    "typescript": "^4.7.4"
  }
}
